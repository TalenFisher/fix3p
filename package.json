{
  "name": "fix3p",
  "version": "0.1.21",
  "description": "Application for fixing headers in X3P bullet scan files",
  "main": "",
  "dependencies": {
    "@talenfisher/canvas": "^0.2.0",
    "@talenfisher/gl-plot3d": "^0.1.4",
    "axios": "^0.18.0",
    "blueimp-md5": "^2.10.0",
    "color-to-vec4": "^0.1.1",
    "file-saver": "^2.0.0",
    "fullscreen-api-polyfill": "^1.1.2",
    "gl-textured-surface3d": "^1.3.9",
    "ify-loader": "^1.1.0",
    "jszip": "^3.1.5",
    "ndarray": "^1.0.18"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^3.3.0",
    "@types/jest": "^23.3.10",
    "@types/jest-environment-puppeteer": "^2.2.1",
    "@types/node": "^10.12.14",
    "@types/puppeteer": "^1.11.1",
    "chrome-webstore-upload-cli": "^1.2.0",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "jest-environment-puppeteer": "^3.7.0",
    "jest-puppeteer": "^3.7.0",
    "node-sass": "^4.11.0",
    "puppeteer": "^1.11.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2",
    "watch": "^1.0.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "build:sass": "node-sass --output-style compressed src/assets/sass/main.scss src/assets/dist/fix3p.bundle.css",
    "build:js": "webpack",
    "build": "npm run build:sass && webpack",
    "watch:sass": "watch 'npm run build:sass' src/assets/sass",
    "watch:js": "watch 'npm run build:js' src/assets/js",
    "watch": "concurrently npm:watch:js npm:watch:sass",
    "release": "webstore upload --source extension.zip --extension-id $WS_EXT_ID --client-id $WS_CLIENT_ID --client-secret $WS_CLIENT_SECRET --refresh-token $WS_REFRESH_TOKEN --auto-publish",
    "start": "http-server -c-2 -p 8080 src -o > /dev/null",
    "test": "jest --colors --silent",
    "develop": "concurrently npm:start npm:watch",
    "postinstall": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "browser": {
    "ws": false
  },
  "author": "Talen Fisher",
  "license": "UNLICENSED",
  "browserify": {
    "transform": [
      "glslify",
      "brfs"
    ]
  }
}
